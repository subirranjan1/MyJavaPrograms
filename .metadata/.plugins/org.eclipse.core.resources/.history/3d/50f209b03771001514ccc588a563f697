
package LinkedList;

public class LinkList{
	public Node head;
	
	public LinkList(){	
	}
	public boolean isEmpty(){
		if(head==null){
			return true;
		}
		else{
			return false;
		}
	}	
	private class Node{
		private Object value;
		private Node next;
		
		private Node(Object obj){
			this.value=obj;
		}
		
		public Node getNext(){
			return next;
		}
		public void setNext(Node node){
			next=node;
		}
		public Object getValue(){
			return value;
		}
	}
}

